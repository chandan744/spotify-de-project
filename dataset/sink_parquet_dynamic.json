{
	"name": "sink_parquet_dynamic",
	"properties": {
		"linkedServiceName": {
			"referenceName": "azure_data_lake",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"p_sink_container": {
				"type": "string"
			},
			"p_sink_folder": {
				"type": "string"
			},
			"p_sink_file": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().p_sink_file",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().p_sink_folder",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().p_sink_container",
					"type": "Expression"
				}
			},
			"compressionCodec": "snappy"
		},
		"schema": []
	}
}